#include <stdio.h>
int main() {
	int t,n,k;
	scanf("%d",&t);
	while(t--){
	    scanf("%d %d",&n,&k);
	    int arr[n];
	    int count = 0;
	    for(int i = 0;i<n;i++){
	        scanf("%d",&arr[i]);
	    }
	    for(int i = 0;i<n;i++){
	        if(arr[i]>k)
	            count++;
	    }
	   printf("%d \n",count);
	   }
	}
